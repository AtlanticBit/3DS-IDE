      <style-class name="perl-StringQX" inherit-style="StringQX" />
      <style-class name="perl-StringQR" inherit-style="StringQR" />
      <style-class name="perl-StringQW" inherit-style="StringQW" />
      <style-class name="perl-PodVerb" inherit-style="PodVerb" />
      <style-class name="perl-GlobalDefault" inherit-style="GlobalDefault" />
      <style-class name="perl-LineNumber" inherit-style="LineNumber" />
      <style-class name="perl-BraceLight" inherit-style="BraceLight" />
