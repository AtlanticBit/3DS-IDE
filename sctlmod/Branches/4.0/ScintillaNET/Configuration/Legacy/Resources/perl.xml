<!-- #endregion -->
</globals>
<style-classes>
<!-- #region "xml-style-classes" -->
      <style-class name="perl-Default" inherit-style="Default" />
      <style-class name="perl-Error" inherit-style="Error" />
      <style-class name="perl-CommentLine" inherit-style="CommentLine" />
      <style-class name="perl-Pod" inherit-style="Pod" />
      <style-class name="perl-Number" inherit-style="Number" />
      <style-class name="perl-Word" inherit-style="Word" />
      <style-class name="perl-String" inherit-style="String" />
      <style-class name="perl-Character" inherit-style="Character" />
      <style-class name="perl-Punctuation" inherit-style="Punctuation" />
      <style-class name="perl-Preprocessor" inherit-style="Preprocessor" />
      <style-class name="perl-Operator" inherit-style="Operator" />
      <style-class name="perl-Identifier" inherit-style="Identifier" />
      <style-class name="perl-Scalar" inherit-style="Scalar" />
      <style-class name="perl-Array" inherit-style="Array" />
      <style-class name="perl-Hash" inherit-style="Hash" />
      <style-class name="perl-SymbolTable" inherit-style="SymbolTable" />
      <style-class name="perl-VariableIndexer" inherit-style="VariableIndexer" />
      <style-class name="perl-Regex" inherit-style="Regex" />
      <style-class name="perl-RegularExpressionSubstitution" inherit-style="RegularExpressionSubstitution" />
      <style-class name="perl-LongQuote" inherit-style="LongQuote" />
      <style-class name="perl-BackTicks" inherit-style="BackTicks" />
      <style-class name="perl-DataSection" inherit-style="DataSection" />
      <style-class name="perl-HereDelimiter" inherit-style="HereDelimiter" />
      <style-class name="perl-HereQ" inherit-style="HereQ" />
