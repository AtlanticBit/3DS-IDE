<?xml version="1.0" encoding="utf-8"?>
<Scintilla xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<globals>
<!-- #region "xml-globals" -->
<!-- #endregion -->
</globals>
<style-classes>
<!-- #region "xml-style-classes" -->
      <style-class name="escript-Default" inherit-style="Default" />
      <style-class name="escript-Comment" inherit-style="Comment" />
      <style-class name="escript-CommentLine" inherit-style="CommentLine" />
      <style-class name="escript-CommentDoc" inherit-style="CommentDoc" />
      <style-class name="escript-Number" inherit-style="Number" />
      <style-class name="escript-Word" inherit-style="Word" />
      <style-class name="escript-String" inherit-style="String" />
      <style-class name="escript-Operator" inherit-style="Operator" />
      <style-class name="escript-Identifier" inherit-style="Identifier" />
      <style-class name="escript-Brace" inherit-style="Brace" />
      <style-class name="escript-Word2" inherit-style="Word2" />
      <style-class name="escript-Word3" inherit-style="Word3" />
      <style-class name="escript-GlobalDefault" inherit-style="GlobalDefault" />
      <style-class name="escript-LineNumber" inherit-style="LineNumber" />
      <style-class name="escript-BraceLight" inherit-style="BraceLight" />
      <style-class name="escript-BraceBad" inherit-style="BraceBad" />
      <style-class name="escript-ControlChar" inherit-style="ControlChar" />
      <style-class name="escript-IndentGuide" inherit-style="IndentGuide" />
      <style-class name="escript-CallTip" inherit-style="CallTip" />
      <style-class name="escript-LastPredefined" inherit-style="LastPredefined" />
      <style-class name="escript-Max" inherit-style="Max" />
<!-- #endregion -->
</style-classes>
<keyword-classes>
<!--  #region "xml-keyword-classes" -->
      <keyword-class name="escript-Primary-keywords-and-identifiers" >
      </keyword-class>
      <keyword-class name="escript-Intrinsic-functions" >
      </keyword-class>
      <keyword-class name="escript-Extended-and-user-defined-functions" >
      </keyword-class>
<!-- #endregion -->
</keyword-classes>
<languages>
<!-- #region "xml-languages" -->
      <language name="escript">
         <lexer name="ESCRIPT" style-bits="5"/>
          <editor-style caret-fore="default-caret-fore" selection-fore="default-selection-fore" selection-back="default-selection-back" />
          <character-class inherit="alphanumeric" ></character-class>
         <file-extensions> *.src *.em</file-extensions>
         <use-keywords>
            <keyword key="0" class="escript-Primary-keywords-and-identifiers"/>
            <keyword key="1" class="escript-Intrinsic-functions"/>
            <keyword key="2" class="escript-Extended-and-user-defined-functions"/>
         </use-keywords>
         <use-styles>
            <style name="Default" class="escript-Default" />
            <style name="Comment" class="escript-Comment" />
            <style name="CommentLine" class="escript-CommentLine" />
            <style name="CommentDoc" class="escript-CommentDoc" />
            <style name="Number" class="escript-Number" />
            <style name="Word" class="escript-Word" />
            <style name="String" class="escript-String" />
            <style name="Operator" class="escript-Operator" />
            <style name="Identifier" class="escript-Identifier" />
            <style name="Brace" class="escript-Brace" />
            <style name="Word2" class="escript-Word2" />
            <style name="Word3" class="escript-Word3" />
            <style name="GlobalDefault" class="escript-GlobalDefault" />
            <style name="LineNumber" class="escript-LineNumber" />
            <style name="BraceLight" class="escript-BraceLight" />
            <style name="BraceBad" class="escript-BraceBad" />
            <style name="ControlChar" class="escript-ControlChar" />
            <style name="IndentGuide" class="escript-IndentGuide" />
            <style name="CallTip" class="escript-CallTip" />
            <style name="LastPredefined" class="escript-LastPredefined" />
            <style name="Max" class="escript-Max" />
         </use-styles>

      </language>
<!-- #endregion -->
</languages>
</Scintilla>
