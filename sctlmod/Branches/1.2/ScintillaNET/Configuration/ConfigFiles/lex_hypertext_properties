var.fontcolor-js=$(color-embedded-js),$(font-js)

# HTML and XML share lexing code so use the same style numbers
# Hypertext Markup Language
lex.hypertext.style.default=fore:#000000,$(font-text)
lex.hypertext.style.tag=fore:#000080
lex.hypertext.style.tagunknown=fore:#FF0000
lex.hypertext.style.attribute=fore:#008080
lex.hypertext.style.attributeunknown=fore:#FF0000
lex.hypertext.style.number=$(color-number)
lex.hypertext.style.doublestring=$(color-string)
lex.hypertext.style.singlestring=$(color-string)
lex.hypertext.style.other=fore:#800080
lex.hypertext.style.comment=fore:#808000,$(font-text-comment)
lex.hypertext.style.entity=fore:#800080,$(font-text)
lex.hypertext.style.tagend=fore:#000080
lex.hypertext.style.xmlstart=fore:#0000FF
lex.hypertext.style.xmlend=fore:#0000FF
lex.hypertext.style.script=fore:#000080
lex.hypertext.style.asp=back:#FFFF00
lex.hypertext.style.aspat=back:#FFDF00
lex.hypertext.style.cdata=back:#FFDF00
lex.hypertext.style.question=fore:#0000FF,back:#FFEFBF
lex.hypertext.style.value=fore:#FF00FF,back:#FFEFFF
lex.hypertext.style.xccomment=fore:#000000,back:#FFFFD0
lex.hypertext.style.sgmldefault=fore:#000080,back:#EFEFFF
lex.hypertext.style.sgmlcommand=fore:#000080,back:#EFEFFF,bold
lex.hypertext.style.sgmldoublestring=fore:#006600,back:#EFEFFF
lex.hypertext.style.sgmlsimplestring=fore:#800000,back:#EFEFFF
lex.hypertext.style.sgmlerror=fore:#993300,back:#EFEFFF
lex.hypertext.style.sgmlspecial=fore:#800000,back:#FF6666
lex.hypertext.style.sgmlentity=fore:#3366FF,back:#EFEFFF
lex.hypertext.style.sgmlcomment=fore:#333333,back:#EFEFFF
lex.hypertext.style.sgmlsgml1stparametercomment=fore:#808000,back:#EFEFFF
lex.hypertext.style.sgmlblockdefault=fore:#000066,back:#CCCCE0

# Some reasonable background colors found in the default Windows palette
# Off White #FFFBF0    Light green #C0DCC0    Light Blue #A6CAF0
lex.hypertext.style.bracelight=fore:#0000FF,notbold,$(font-text)
lex.hypertext.style.bracebad=fore:#FF0000,notbold,$(font-text)

# Embedded Javascript
lex.hypertext.style.javascriptstart=fore:#7F7F00
lex.hypertext.style.javascriptdefault=fore:#000000,bold,$(fontcolor-js),eolfilled
lex.hypertext.style.javascriptcomment=$(color-code-comment-box),$(fontcolor-js),eolfilled
lex.hypertext.style.javascriptcommentline=$(color-code-comment-line),$(fontcolor-js)
lex.hypertext.style.javascriptcommentdoc=$(color-code-comment-doc),bold,$(fontcolor-js),eolfilled
lex.hypertext.style.javascriptnumber=$(color-number),$(fontcolor-js)
lex.hypertext.style.javascriptword=fore:#000000,$(fontcolor-js)
lex.hypertext.style.javascriptkeyword=$(color-keyword),bold,$(fontcolor-js)
lex.hypertext.style.javascriptdoublestring=$(color-string),$(fontcolor-js)
lex.hypertext.style.javascriptsinglestring=$(color-string),$(fontcolor-js)
lex.hypertext.style.javascriptsymbols=$(color-operator),bold,$(fontcolor-js)
lex.hypertext.style.javascriptstringeol=back:#BFBBB0,eolfilled
lex.hypertext.style.javascriptregex=back:#FFBBB0

# ASP Javascript
lex.hypertext.style.aspjavascriptstart=fore:#7F7F00
lex.hypertext.style.aspjavascriptdefault=fore:#000000,bold,$(font-js),back:#DFDF7F,eolfilled
lex.hypertext.style.aspjavascriptcomment=fore:#007F00,$(font-js),back:#DFDF7F,eolfilled
lex.hypertext.style.aspjavascriptcommentline=fore:#007F00,$(font-js),back:#DFDF7F
lex.hypertext.style.aspjavascriptcommentdoc=fore:#7F7F7F,bold,$(font-js),back:#DFDF7F,eolfilled
lex.hypertext.style.aspjavascriptnumber=fore:#007F7F,$(font-js),back:#DFDF7F
lex.hypertext.style.aspjavascriptword=fore:#000000,$(font-js),back:#DFDF7F
lex.hypertext.style.aspjavascriptkeyword=fore:#00007F,bold,$(font-js),back:#DFDF7F
lex.hypertext.style.aspjavascriptdoublestring=fore:#7F007F,$(font-js),back:#DFDF7F
lex.hypertext.style.aspjavascriptsinglestring=fore:#7F007F,$(font-js),back:#DFDF7F
lex.hypertext.style.aspjavascriptsymbols=fore:#000000,bold,$(font-js),back:#DFDF7F
lex.hypertext.style.aspjavascriptstringeol=back:#BFBBB0,eolfilled
lex.hypertext.style.aspjavascriptregex=back:#FFBBB0

# Embedded VBS
lex.hypertext.style.vbscriptstart=
lex.hypertext.style.vbscriptdefault=$(font-vbs),back:#EFEFFF,fore:#000000,eolfilled
lex.hypertext.style.vbscriptcommentline=$(font-comment),back:#EFEFFF,fore:#008000,eolfilled
